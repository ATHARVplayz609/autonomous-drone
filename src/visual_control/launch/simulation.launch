<launch>
  <include file="$(find px4)/launch/mavros_posix_sitl.launch" />
  <node name="whycon" type="whycon" pkg="whycon" output="screen">
    <remap from="camera/image_rect_color" to="camera/rgb/image_rect_color" />
    <remap from="camera/camera_info" to="camera/rgb/camera_info" />
    <param name="targets" value="1" />
    <param name="inner_diameter" value="0.09" />
    <param name="outer_diameter" value="0.217" />
  </node>
  <node name="image_view" type="image_view" pkg="image_view" output="screen" >
    <remap from="image" to="whycon/image_out" />
  </node>
  <node name="takeoff_n_land" type="takeoff_n_land" pkg="visual_control" output="screen" />
</launch>
